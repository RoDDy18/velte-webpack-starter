{
    "name": "velte-webpack-starter",
    "version": "1.0.0",
    "description": "Velte starter kit powered by webpack",
    "scripts": {
        "dev": "webpack-dev-server --config webpack.config.js --mode development",
        "build": "rm -rf dist && webpack --config webpack.config.js --mode production",
        "preview": "npm run build && http-server dist",
        "lint": "eslint . --ext .js,.jsx",
        "lint:fix": "eslint . --ext .js,.jsx --fix"
    },
    "dependencies": {
        "@kodnificent/sparouter": "^1.1.0",
        "valtio": "^1.10.5",
        "velte": "^1.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.22.5",
        "@babel/helper-module-imports": "^7.22.5",
        "@babel/plugin-transform-react-jsx": "^7.22.5",
        "@babel/preset-env": "^7.22.5",
        "@egoist/tailwindcss-icons": "^1.1.0",
        "@iconify-json/uil": "^1.1.4",
        "autoprefixer": "^10.4.14",
        "babel-loader": "^9.1.2",
        "babel-plugin-jsx-control-statements": "^4.1.2",
        "babel-plugin-loop-optimizer": "^1.4.1",
        "babel-plugin-macros": "^3.1.0",
        "eslint": "^8.37.0",
        "eslint-plugin-react": "^7.32.2",
        "css-loader": "^6.7.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "http-server": "^14.1.0",
        "normalize.css": "^8.0.1",
        "postcss": "^8.4.21",
        "postcss-loader": "^7.3.3",
        "postcss-preset-env": "^8.4.2",
        "rimraf": "^3.0.2",
        "sass": "^1.52.1",
        "sass-loader": "^13.0.0",
        "source-map-loader": "^3.0.1",
        "style-loader": "^3.3.1",
        "svg-inline-loader": "^0.8.2",
        "tailwindcss": "^3.3.1",
        "webpack": "^5.86.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1"
    }
}
